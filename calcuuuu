  String storeValues(String value){
    String last_val = "";

    if (equation.length > 1) {
      last_val = equation[equation.length - 1];

      if (last_val == RegExp(r'[+\-*/]') && value == RegExp(r'[+\-*/]')) {
        value += "";
      }
    }


    
    print("Last Equation: " + last_val);

    if (value == "/") {
      value = "รท";
    }
    equation += value;
    print("Display: " + equation);
    return equation;
  }
