  void convertToPercentage() { 
    if (number1.isNotEmpty && operand.isNotEmpty && number2.isNotEmpty) {
      // calculate before conversion
      // TODO
  
      calculate();
    }

    if (operand.isNotEmpty) {
      // cannot be converted
      return;
    }
    
    if (number1 != null) {

          final number = double.parse(number1);
    setState(() {
      number1 = "${(number / 100)}";
      operand = "";
      number2 = "";
    });
    }

  }
